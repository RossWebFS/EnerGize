{
  "expo": {
    "name": "energize",
    "slug": "energize",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "scheme": "energize",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ser.ross.energize"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ser.ross.energize"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-asset",
      "expo-splash-screen",
      "react-native-edge-to-edge",
      "expo-secure-store",
      [
        "expo-notifications",
        {
          "color": "#ffffff",
          "mode": "production"
        }
      ],
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "0e0926d8-1ebe-49a7-aef6-30bdc53c9dc7"
      }
    },
    "owner": "ser.ross"
  }
}
